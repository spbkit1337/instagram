{
  "manifest_version": 2,
  "name": "Убрать фон инсты",
  "description": "Просто убирает черный фон и окно входа",
  "version": "0.0.1",
  "author": "Хвалимов Максим",
  "browser_action": {
    "default_title": "Удаление фона и окна входа",
    "default_icon": "iconfinder-social-media-applications-3instagram-4102579_113804.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.min.js", "content.js", "changeBg.js"]
    }
  ],
  "permissions": ["activeTab"]
}
